<Window x:Class="Desctop.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Desctop"
        xmlns:uc="clr-namespace:Desctop.Controls"
        mc:Ignorable="d"
        Title="DragAndDrop" Height="450" Width="800"
        MinHeight="450" MinWidth="800">
    <Grid>
        <TabControl>
            <TabItem Header="Items">
                <uc:ItemControl></uc:ItemControl>
            </TabItem>
            <TabItem Header="StackPanel">
                <uc:StackControl></uc:StackControl>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
