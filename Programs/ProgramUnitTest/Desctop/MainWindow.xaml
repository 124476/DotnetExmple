<Window x:Class="Desctop.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Desctop"
        mc:Ignorable="d"
        Title="UnitTest" Height="450" Width="800"
         MinHeight="450" MinWidth="800">
    <Grid>
        <StackPanel Width="200" HorizontalAlignment="Center" VerticalAlignment="Center">
            <TextBlock>Хэш</TextBlock>
            <TextBox Name="TextHech"></TextBox>
            <TextBlock>Текст</TextBlock>
            <TextBox Name="TextText"></TextBox>
            <Button x:Name="BtnHech" Click="BtnHech_Click">Захэшировать</Button>
        </StackPanel>
    </Grid>
</Window>
